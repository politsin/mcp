{
  "name": "politsin/mcp",
  "description": "PHP-библиотека для взаимодействия с MCP (Model Context Protocol)",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "mcp",
    "ai",
    "symfony",
    "php"
  ],
  "require": {
    "php": "^8.3",
    "react/http": "^1.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^11"
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage"
  },
  "autoload": {
    "psr-4": {
      "Politsin\\Mcp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Politsin\\Mcp\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/politsin/mcp/issues",
    "source": "https://github.com/politsin/mcp"
  },
  "suggest": {
    "symfony/console": "Для регистрации команды app:react в приложении Symfony"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
      "version": "1.0.27",
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}
